      <key name="action" datatype=".warn_action" default="default"/>
      <key name="message" default=""/>
      <key name="category" datatype=".warn_category" default="Warning"/>
      <key name="module" default=""/>
      <key name="lineno" datatype="integer" default="0"/>
  </sectiontype>
</component>
